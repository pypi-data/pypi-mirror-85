{
	"app": {
		"rpi_health": {
			"enabled": false,
			"config": {
				"interval": 10
			}
		},
		"http_api": {
			"enabled": false,
			"config": {
				"listen": "0.0.0.0",
				"port": 5555
			}
		},
		"mqtt_bridge": {
			"enabled": false
		}
	},
	"core": {
		"state_updater": {
			"enabled": true
		},
		"resource_worker": {
			"enabled": true,
			"worker_count": 1
		},
		"data_kernel": {
			"enabled": true,
			"config": {
				"buffer_max_age": 60,
				"buffer_max_size": 100,
				"vacuum_interval": 10,
				"port": 6003
			}
		}
	},
	"storage": {
		"json": {
			"jsqlite": {
				"worker_count": 2
			}
		},
		"series": {
			"ssqlite": {
				"worker_count": 4
			}
		}
	},
	"topology": {
		"upstream": {
			"upstream-node": {
				"client_key": "gateway",
				"server_key": "server",
				"hostname": "ggm.server",
				"port": 6001
			}
		},
		"downstream": {
			"private-api": {
				"server_key": "gateway",
				"port": 6001
			}
		}
	},
	"global_options": {
		"verbose": true,
		"cache": false,
		"resource_filter": false
	}
}
