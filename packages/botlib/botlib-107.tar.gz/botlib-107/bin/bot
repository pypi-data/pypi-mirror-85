#!/usr/bin/python3
"library for programming objects"

import os, sys
sys.path.insert(0, "lib")
sys.path.insert(0, "bot")

from csl import Console
from prs import parse_cli
from trm import execute, os

def main():
    cfg = parse_cli("bot")
    c = Console()
    c.scan("irc")
    if cfg.txt:
       return c.cmd(cfg.txt)
    c.init(cfg.mods)
    c.start()
    c.wait()
     
execute(main)
os._exit(0)
