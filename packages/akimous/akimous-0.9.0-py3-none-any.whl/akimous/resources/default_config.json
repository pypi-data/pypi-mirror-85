{
  "formatter": {
    "realtime": true,
    "isort": true,
    "yapf": true
  },
  "linter": {
    "pyflakes": true,
    "pylint": true,
    "spellChecker": true
  },
  "offlineLintType": {
    "informational": true,
    "refactor": true,
    "convention": true,
    "warning": true,
    "error": true
  },
  "macro": {
    "macroFormatWithShift": "${NAME} = "
  },
  "keymap": {
    "layeredKeyboardControl": true,
    "keymapHint": false
  },
  "lastOpenedFolder": null,
  "panelLeft": {
    "hidden": false,
    "width": 0
  },
  "panelRight": {
    "hidden": false,
    "width": 0
  },
  "editor": {
    "lineWrapping": false
  }
}