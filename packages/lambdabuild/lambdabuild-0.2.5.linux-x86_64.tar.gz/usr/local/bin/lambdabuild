#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'lambdabuild==0.2.5','console_scripts','lambdabuild'
__requires__ = 'lambdabuild==0.2.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('lambdabuild==0.2.5', 'console_scripts', 'lambdabuild')()
    )
